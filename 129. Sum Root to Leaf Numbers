class Solution {
public:
    void dfs(TreeNode* root,vector<int> &res,int sum){
        if(root==NULL){
            return;
        }
        sum=sum*10+root->val;
        if((root->left==NULL)&&(root->right==NULL)){
            res.push_back(sum);
        }
        dfs(root->left,res,sum);
        dfs(root->right,res,sum);
    }
    int sumNumbers(TreeNode* root) {
        vector<int> res;
        dfs(root,res,0);
        int sum=0;
        for(auto x:res){
            sum+=x;
        }
        return sum;
    }
};
