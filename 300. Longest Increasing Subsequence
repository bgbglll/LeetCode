class Solution {
public:
    int lengthOfLIS(vector<int>& nums) {
        int n=nums.size();
        if(n==0)return 0;
        int *dp=new int[n];
        for(int i=0;i<n;i++){
            dp[i]=1;
        }
        int res=1;
        for(int i=0;i<n;i++){
            for(int j=i+1;j<n;j++){
                if(nums[j]>nums[i]){
                    dp[j]=max(dp[j],dp[i]+1);
                    if(dp[j]>res){
                        res=dp[j];
                    }
                }
            }
        }
        return res;
    }
};
