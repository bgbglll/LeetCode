int bulbSwitch(int n) {
	int *a = new int[n+1];
	for (int i = 1; i <= n; i++){
		a[i] = 1;
	}
	int i = 2;
	while (i <= n){
		if (i == n){
			a[n] = 1 - a[n];
			break;
		}
		for (int j=i; j <= n; j += i){
			a[j] = 1 - a[j];
		}
		i++;
	}
	int sum=0;
	for (int k = 1; k <= n; k++){
		sum += a[k];
	}
	delete []a;
	return sum;
}

FUCK!
int bulbSwitch(int n)
{
    return sqrt(n);
}
