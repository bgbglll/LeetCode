class Solution {
public:
    void dfs(TreeNode* root,int level,int &max,vector<int> &res){
        if(root==NULL)return;
        if(level>max){
            max=level;
            res.push_back(root->val);
        }
        dfs(root->right,level+1,max,res);
        dfs(root->left,level+1,max,res);
    }
    vector<int> rightSideView(TreeNode* root) {
        int max=0;
        vector<int> res;
        dfs(root,1,max,res);
        return res;
    }
};
