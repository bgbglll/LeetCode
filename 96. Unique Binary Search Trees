class Solution {//树都总数为左子树的个数*右子数
public:
    int numTrees(int n) {
        vector<int> res;
        res.push_back(1);
        for(int i=1;i<=n;i++){
            res.push_back(0);
            if(i<2)res[i]=i;
            else{
                for(int j=1;j<=i;j++){
                    res[i]+=res[j-1]*res[i-j];
                }
            }
        }
        return res[n];
    }
};
